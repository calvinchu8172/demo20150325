<h1>Event List</h1>
<%= paginate @events %>
<% @events.each do |event| %>
  <ul>
    <li>
        <%= event.name %>
        <%= link_to 'Show', event_path(event) %>
        <%= link_to 'Edit', edit_event_path(event) %>
        <%= link_to 'Delete', event_path(event), :method => :delete %>
    </li>
  </ul>
<%end%>
<%= paginate @events %>
<%= link_to 'New Event', new_event_path%>
<%= link_to 'Home', root_path %>